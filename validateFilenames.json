{
  "rules": [
    {
      "validation": "camelCase",
      "patterns": ["**/*"]
    },
    {
      "validation": "PascalCase",
      "patterns": ["**/src/ClownFilesystem.ts"]
    },
    {
      "validation": "ignore",
      "patterns": [
        "**/typings/**/*",
        "**/__tests__/**/*",
        "**/__mocks__/**/*",
        "**/__snapshots__/**/*",
        "**/docker-compose.yml",
        "**/Dockerfile*",
        "**/LICENSE",
        "**/README.md",
        "**/src/cli/**",
        "**/c__*"
      ]
    }
  ]
}
